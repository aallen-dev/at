    "@babel/preset-typescript": "^7.12.7",
    "@types/puppeteer": "^5.4.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "browserify": "^17.0.0",
    "core-js": "^3.7.0",
    "terser": "^5.5.0",
    "typescript": "^4.1.2",
    "watchify": "^3.11.1"
  },
